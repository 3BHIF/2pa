package Cities;

import java.util.TreeMap;

/**
 *
 * @author lessi
 */
public class City {
    public TreeMap<String, String> cities = new TreeMap<>();
    
    public void initMap() {
        cities.put("Wien", "Meidling");
        cities.put("Wiener Neustadt", "Hbf");
        cities.put("Gloggnitz", "Hauptbahnhof");
    }
    
    public String getValues(String key) {
        String ret = cities.get(key);
        return ret;
    }
}

